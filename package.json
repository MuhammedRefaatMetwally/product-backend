    "express": "4.21.2",
    "helmet": "8.1.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.1",
    "prisma": "6.16.2",
    "winston": "3.17.0",
    "zod": "3.25.76",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/cookie-parser": "1.4.9",
    "@types/cors": "2.8.19",
    "@types/express": "4.17.23",
    "@types/jsonwebtoken": "9.0.10",
    "@types/morgan": "1.9.10",
